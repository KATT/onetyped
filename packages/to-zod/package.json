{
  "name": "@onetyped/to-zod",
  "version": "0.0.0",
  "type": "module",
  "description": "onetyped schema to Zod schema",
  "main": "dist/index.cjs",
  "exports": {
    "./package.json": "./package.json",
    "./schema": {
      "import": "./dist/schema.js",
      "default": "./dist/schema.cjs"
    },
    "./string": {
      "import": "./dist/string.js",
      "default": "./dist/string.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc"
  },
  "sideEffects": false,
  "dependencies": {
    "@onetyped/core": "workspace:0.0.0"
  },
  "devDependencies": {
    "zod": "3.18.0"
  },
  "peerDependencies": {
    "zod": "^3"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  }
}
