{
  "name": "root",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "pnpm -r type-check",
    "lint:js": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm build",
    "test": "vitest"
  },
  "devDependencies": {
    "@sachinraja/eslint-config": "0.1.1",
    "@types/node": "18.7.3",
    "dprint": "0.30.3",
    "eslint": "8.22.0",
    "husky": "8.0.1",
    "nano-staged": "0.8.0",
    "npm-run-all": "4.1.5",
    "tsup": "6.2.2",
    "tsx": "3.8.2",
    "typescript": "4.7.4",
    "vite": "3.0.7",
    "vitest": "0.21.1"
  },
  "sideEffects": false,
  "nano-staged": {
    "*.{js,ts,md,json,yaml}": "dprint fmt"
  }
}
